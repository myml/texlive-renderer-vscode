{
	"name": "texlive-remote-render",
	"publisher": "myml",
	"displayName": "LuaTex renderer",
	"description": "Use remote server render tex file",
	"version": "0.0.2",
	"engines": {
		"vscode": "^1.52.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onCommand:texlive-remote-render.texlive_render"
	],
	"repository": {
		"type": "git",
		"url": "https://github.com/myml/texlive-renderer-vscode.git"
	},
	"main": "./out/extension.js",
	"contributes": {
		"keybindings":[
			{
				"command": "texlive-remote-render.texlive_render",
				"key": "alt+d",
				"when":"resourceExtname==.tex"
			}
		],
		"commands": [
			{
				"command": "texlive-remote-render.texlive_render",
				"title": "luatatex preview"
			}
		],
		"configuration":{
			"title": "Texlive renderer",
			"properties": {
				"server":{
					"type": "string",
					"default": "http://localhost:8080",
					"description": "render server"
				}
			}
		}
	},
	"scripts": {
		"vscode:prepublish": "yarn run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "yarn run compile && yarn run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.3",
		"@types/mocha": "^8.0.4",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.52.0",
		"@typescript-eslint/eslint-plugin": "^4.9.0",
		"@typescript-eslint/parser": "^4.9.0",
		"eslint": "^7.15.0",
		"glob": "^7.1.6",
		"mocha": "^8.1.3",
		"typescript": "^4.1.2",
		"vscode-test": "^1.4.1"
	},
	"dependencies": {
		"fetch": "^1.1.0"
	},
	"extensionDependencies":["tomoki1207.pdf"]
}
